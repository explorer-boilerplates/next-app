{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "emotion": "^10.0.27",
    "emotion-server": "^10.0.27",
    "next": "9.3.6",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@storybook/addon-a11y": "^6.0.0-beta.4",
    "@storybook/addon-actions": "^6.0.0-beta.4",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-docs": "^6.0.0-beta.4",
    "@storybook/addon-links": "^6.0.0-beta.4",
    "@storybook/addon-viewport": "^6.0.0-beta.4",
    "@storybook/addons": "^6.0.0-beta.4",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.0.0-beta.4",
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.34",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-macros": "^2.8.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-emotion": "^10.0.27",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-unicorn": "^19.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "twin.macro": "^1.0.0-alpha.10",
    "typescript": "^3.8.3",
    "typescript-plugin-tw-template": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,md,mdx}": [
      "prettier --write .",
      "eslint --fix --max-warnings=0 ."
    ],
    "./**/*.json": [
      "prettier --write ."
    ]
  },
  "babelMacros": {
    "twin": {
      "config": "./tailwind.config.js",
      "styled": "@emotion/styled",
      "format": "auto",
      "hasSuggestions": true,
      "debug": false
    }
  }
}
